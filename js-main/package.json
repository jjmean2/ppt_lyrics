{
  "name": "js-main",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jongwan Lee <jjmean2@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "console-table-printer": "^2.7.5",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "lodash": "^4.17.20",
    "ts-enum-util": "^4.0.2"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0 --ext .ts,.js",
    "lint:fix": "eslint . --max-warnings 0 --ext .ts,.js --fix",
    "format": "prettier 'src/**/*.ts' index.js --write --config .prettierrc --ignore-path .prettierignore",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "start:dev": "NODE_ENV=development node index.js",
    "build": "babel src --out-file lib/main.js --copy-files --extensions '.ts,.js'",
    "build:tsc": "tsc --rootDir src --outDir lib --noEmit false",
    "start": "node index.js",
    "buildAndRun": "yarn build && ./bin/ppt-create"
  }
}
